{
  "name": "x-camp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix && git add . && next lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write ./src",
    "lint:style": "stylelint \"./src/**/*.{css,scss}\" --fix --cache --cache-location node_modules/.cache/stylelint/ --ignore-path .gitignore"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.2",
    "ahooks": "^3.7.7",
    "animate.css": "^4.1.1",
    "antd": "^5.5.1",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "eruda": "^3.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "^14.0.4",
    "hover.css": "^2.3.2",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "randomcolor": "^0.6.2",
    "react": "18.2.0",
    "react-cookies": "^0.1.1",
    "react-countup": "^6.4.2",
    "react-dom": "18.2.0",
    "react-intl": "^6.4.3",
    "react-router-dom": "^6.14.1",
    "react-slick": "^0.29.0",
    "sass": "1.62.1",
    "typescript": "5.0.4",
    "unified": "^10.1.2",
    "x-star-editor": "^0.1.2",
    "x-star-utils": "^0.0.10"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.2.3",
    "@types/randomcolor": "^0.5.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/react-scroll": "^1.8.10",
    "eslint-config-x-star": "^1.0.5",
    "lint-staged": "^14.0.1",
    "postcss-pxtorem": "^6.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.2.1",
    "unplugin-json-dts": "^1.2.0"
  }
}
